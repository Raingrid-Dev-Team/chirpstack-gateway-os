Index: openwrt/target/linux/ramips/dts/mt7628an_rakwireless_rak636.dts
===================================================================
--- /dev/null
+++ openwrt/target/linux/ramips/dts/mt7628an_rakwireless_rak636.dts
@@ -0,0 +1,175 @@
+// SPDX-License-Identifier: GPL-2.0-or-later OR MIT
+
+#include "mt7628an.dtsi"
+
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/input/input.h>
+
+/ {
+	compatible = "rakwireless,rak636", "mediatek,mt7628an-soc";
+	model = "Rakwireless RAK636";
+
+	leds {
+		compatible = "gpio-leds";
+		lora {
+			label = "rak:green:lora";
+			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
+		};
+		ethernet {
+			label = "rak:green:eth";
+			gpios = <&gpio 43 GPIO_ACTIVE_LOW>;
+		};
+		lora1 {
+			label = "rak:green:lora1";
+			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
+		};
+
+		wifi {
+			label = "mediatek:orange:wifi";
+			gpios = <&gpio 44 GPIO_ACTIVE_LOW>;
+		};
+	};
+
+    keys {
+		compatible = "gpio-keys-polled";
+
+		#address-cells = <1>;
+		#size-cells = <0>;
+		poll-interval = <20>;
+
+		reset {
+			label = "reset";
+			gpios = <&gpio 38 1>;
+			linux,code = <0x198>;
+		};
+
+		dying-gasp {
+			label = "dying-gasp";
+			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
+			linux,code = <BTN_7>;
+		};
+	};
+};
+
+&state_default {
+	i2s {
+		groups = "i2s";
+		function = "gpio";
+	};
+	spis {
+		groups = "spis";
+		function = "gpio";
+	};
+	p0led_an {
+		groups = "p0led_an";
+		function = "gpio";
+	};
+	p1led_an {
+		groups = "p1led_an";
+		function = "gpio";
+	};
+	p2led_an {
+		groups = "p2led_an";
+		function = "gpio";
+	};
+	p3led_an {
+		groups = "p3led_an";
+		function = "gpio";
+	};
+	p4led_an {
+		groups = "p4led_an";
+		function = "gpio";
+	};
+	wled_an {
+		groups = "wled_an";
+		function = "gpio";
+	};
+};
+
+&spi0 {
+	status = "okay";
+
+	pinctrl-names = "default";
+	pinctrl-0 = <&spi_pins>, <&spi_cs1_pins>;
+
+	flash@0 {
+		compatible = "jedec,spi-nor";
+		reg = <0>;
+		spi-max-frequency = <10000000>;
+
+		partitions {
+			compatible = "fixed-partitions";
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			partition@0 {
+				label = "u-boot";
+				reg = <0x0 0x30000>;
+				read-only;
+			};
+
+			partition@30000 {
+				label = "u-boot-env";
+				reg = <0x30000 0x10000>;
+				read-only;
+			};
+
+			factory: partition@40000 {
+				label = "factory";
+				reg = <0x40000 0xe000>;
+			};
+
+			partition@4e000 {
+				label = "pst-data"; /* persistent data */
+				reg = <0x4e000 0x2000>;
+			};
+
+			partition@50000 {
+				compatible = "denx,uimage";
+				label = "firmware";
+				reg = <0x50000 0x1fb0000>;	/* support 32MB norflash */
+			};
+		};
+	};
+
+	spidev@1 {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		compatible = "spidev";
+		reg = <1>;
+		spi-max-frequency = <40000000>;
+	};
+};
+
+&i2c {
+	status = "okay";
+};
+
+&i2s {
+	status = "disabled";
+};
+
+&uart1 {
+	status = "okay";
+};
+
+&uart2 {
+	status = "okay";
+};
+
+&gdma {
+	status = "okay";
+};
+
+&ethernet {
+	mtd-mac-address = <&factory 0x4>;
+};
+
+&sdhci {
+	status = "okay";
+	mediatek,cd-poll;
+};
+
+&wmac {
+	status = "okay";
+};
